{
  "name": "quick-select",
  "version": "1.0.10",
  "description": "quick-select",
  "main": "dist/quickselect.min.js",
  "scripts": {
    "build:babel": "npm run clear && npm run build:babel:umd && npm run build:babel:min",
    "build:babel:umd": "babel --no-babelrc src/QuickSelect-js.js -o ./dist/quickselect.js --presets=es2015,stage-0 --plugins=add-module-exports,transform-es2015-modules-umd",
    "build:babel:min": "babel --no-babelrc src/QuickSelect-js.js -o ./dist/quickselect.min.js --presets=es2015,stage-0 --plugins=add-module-exports,transform-es2015-modules-umd --compact=true",
    "build": "webpack",
    "clear": "rm -rf dist && mkdir dist",
    "dev": "webpack-dashboard -- nodemon --config nodemon.json ./server.js",
    "test": "mocha",
    "test:karma-my": "karma start my.conf.js",
    "test:karma": "karma start karma.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/linshfu/quick-select.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/linshfu/quick-select/issues"
  },
  "homepage": "https://github.com/linshfu/quick-select#readme",
  "dependencies": {
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-modules-umd": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.5.0",
    "browser-sync": "^2.14.0",
    "chai": "^3.5.0",
    "css-loader": "^0.26.1",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.5.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-commonjs": "^1.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-requirejs": "^1.1.0",
    "karma-webpack": "^2.0.2",
    "mocha": "^3.2.0",
    "node-sass": "^4.0.0",
    "nodemon": "^1.11.0",
    "phantomjs-prebuilt": "^2.1.14",
    "pug": "2.0.0-beta6",
    "pug-loader": "2.3.0",
    "requirejs": "^2.3.3",
    "sass-loader": "^6.0.0",
    "style-loader": "^0.13.1",
    "vue-hot-reload-api": "^2.0.10",
    "vue-loader": "^11.1.2",
    "vue-router": "^2.2.1",
    "vue-template-compiler": "^2.2.0-beta.1",
    "webpack": "^2.3.1",
    "webpack-bundle-analyzer": "^2.2.2",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.16.1"
  }
}
